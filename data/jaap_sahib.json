{
  "title": "Jaap Sahib",
  "verses": [
    {
      "line_number": 1,
      "gurmukhi": "ੴ ਸਤਿਗੁਰ ਪ੍ਰਸਾਦਿ ॥",
      "transliteration": "Ik oankaar satigur prasaadh ||",
      "english": "One Universal Creator God. By The Grace Of The True Guru."
    },
    {
      "line_number": 2,
      "gurmukhi": "ਸ੍ਰੀ ਵਾਹਿਗੁਰੂ ਜੀ ਕੀ ਫਤਹ ॥",
      "transliteration": "sree vaahiguroo jee kee fateh ||",
      "english": "Victory to the Glorious Wondrous Guru."
    },
    {
      "line_number": 3,
      "gurmukhi": "ਜਾਪੁ ॥",
      "transliteration": "jaap ||",
      "english": "Chant."
    },
    {
      "line_number": 4,
      "gurmukhi": "ਸ੍ਰੀ ਮੁਖਵਾਕ ਪਾਤਿਸਾਹੀ ੧੦ ॥",
      "transliteration": "sree mukhavaak paatisaahee 10 ||",
      "english": "Utterance of the Tenth Sovereign."
    }
  ]
}
